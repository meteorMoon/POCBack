    "build": "tsc",
    "start": "ts-node-dev --respawn src/index.ts",
    "test": "ts-node .\\node_modules\\jasmine\\bin\\jasmine",
    "lint": "eslint .  --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test:coverage": "npm run cover:unit && npm run cover:report",
    "test:coveragehtml": "npm run cover:unit && npm run cover:reporthtml",
    "cover:unit": "nyc npm run test",
    "cover:report": "nyc report --reporter=lcov --reporter=text --report-dir='./jscoverage' --statements 90 --functions 90 --lines 90",
    "cover:reporthtml": "nyc report --reporter=html --report-dir='./jscoverage' --statements 90 --functions 90 --lines 90"
  },
